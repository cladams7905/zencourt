{
  "name": "@zencourt/video-server",
  "version": "1.0.0",
  "private": true,
  "description": "Express server for video processing and Backblaze B2 storage (Hetzner compatible)",
  "main": "dist/apps/video-server/src/server.js",
  "scripts": {
    "dev": "bash scripts/start-dev-server.sh",
    "build": "tsc",
    "start": "node dist/apps/video-server/src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "docker:rebuild": "docker compose --env-file ../../.env.local down && docker compose --env-file ../../.env.local build video-server && docker compose --env-file ../../.env.local up -d",
    "docker:up": "docker compose --env-file ../../.env.local up -d",
    "docker:down": "docker compose --env-file ../../.env.local down",
    "docker:logs": "docker compose logs -f video-server"
  },
  "keywords": [
    "video",
    "processing",
    "express",
    "aws"
  ],
  "author": "Zencourt",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@fal-ai/client": "^1.7.2",
    "@remotion/bundler": "^4.0.0",
    "@remotion/google-fonts": "^4.0.419",
    "@remotion/renderer": "^4.0.0",
    "@runwayml/sdk": "^3.11.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.7.9",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "remotion": "^4.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "fast-xml-parser": "^5.3.4"
  }
}
