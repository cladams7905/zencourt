{
  "name": "@zencourt/video-server",
  "version": "1.0.0",
  "private": true,
  "description": "Express server for FFmpeg video processing and S3 storage on AWS ECS",
  "main": "dist/apps/video-server/src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/apps/video-server/src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "docker:rebuild": "docker compose --env-file ../../.env.local down && docker compose --env-file ../../.env.local build video-server && docker compose --env-file ../../.env.local up -d",
    "docker:up": "docker compose --env-file ../../.env.local up -d",
    "docker:down": "docker compose --env-file ../../.env.local down",
    "docker:logs": "docker compose logs -f video-server"
  },
  "keywords": [
    "video",
    "processing",
    "ffmpeg",
    "express",
    "aws"
  ],
  "author": "ZenCourt",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.699.0",
    "@aws-sdk/s3-request-presigner": "^3.699.0",
    "@fal-ai/client": "^1.7.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^2.0.0",
    "axios": "^1.7.9",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/ffmpeg-static": "^3.0.3",
    "@types/ffprobe-static": "^2.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
