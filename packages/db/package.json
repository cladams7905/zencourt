{
  "name": "@zencourt/db",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "dotenv -e ../../.env.local -- drizzle-kit generate --config=drizzle/drizzle.config.ts",
    "db:migrate": "dotenv -e ../../.env.local -- drizzle-kit migrate --config=drizzle/drizzle.config.ts",
    "db:push": "dotenv -e ../../.env.local -- drizzle-kit push --config=drizzle/drizzle.config.ts",
    "db:studio": "dotenv -e ../../.env.local -- drizzle-kit studio --config=drizzle/drizzle.config.ts",
    "lint": "eslint .",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@stackframe/stack": "^2.8.63",
    "@stackframe/stack-shared": "^2.8.63"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@types/jest": "^29.5.14",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5"
  },
  "overrides": {
    "esbuild": "^0.25.12"
  }
}
