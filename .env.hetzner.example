# Server
NODE_ENV=production
PORT=3001
LOG_LEVEL=info

# Backblaze B2 object storage
B2_ENDPOINT=https://s3.us-west-002.backblazeb2.com
B2_REGION=us-west-002
B2_KEY_ID=your-key-id
B2_APPLICATION_KEY=your-application-key
B2_BUCKET_NAME=zencourt-media-prod

# URLs
VIDEO_SERVER_URL=https://video.zencourt.com
VERCEL_API_URL=https://zencourt.com

# Auth / secrets
VIDEO_SERVER_API_KEY=replace-with-shared-secret
VERCEL_WEBHOOK_SIGNING_KEY=replace-with-vercel-webhook-secret
DATABASE_URL=postgres://user:pass@host:5432/db

# fal.ai
FAL_KEY=replace-with-fal-key
FAL_WEBHOOK_URL=https://video.zencourt.com/webhooks/fal

# Processing
TEMP_DIR=/tmp/video-processing
MAX_CONCURRENT_JOBS=1
JOB_TIMEOUT_MS=600000
